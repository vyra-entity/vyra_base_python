{
    "version": 1,
    "disable_existing_loggers": 0,
    
    "loggers": {
      "vyra_base": {
        "level": "DEBUG",
        "handlers": ["file_handler"]
      }
    },
    
    "formatters": {
      "debug": {
        "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
      }
    },
  
    "handlers": {
      "file_handler": {
        "class": "logging.handlers.RotatingFileHandler",
        "level": "DEBUG",
        "formatter": "debug",
        "filename": "log/vyra/vyra_main_stdout.log",
        "mode": "a",
        "maxBytes": 5242880,
        "backupCount": 40,
        "encoding": "utf8",
        "delay": 0
      }
    }
  }
