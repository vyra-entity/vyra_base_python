{
    "version": 1,
    "disable_existing_loggers": false,
    "incremental": false,
    
    "loggers": {
      "vyra_base": {
        "level": "DEBUG",
        "handlers": ["file_handler"],
        "propagate": false
      }
    },
    "formatters": {
      "debug": {
        "format": "%(asctime)s - %(levelname)-8s - %(name)s - %(message)s"
      }
    },
    "handlers": {
      "file_handler": {
        "class": "logging.handlers.RotatingFileHandler",
        "level": "DEBUG",
        "formatter": "debug",
        "filename": "log/vyra/vyra_main_stdout.log",
        "mode": "a",
        "maxBytes": 5242880,
        "backupCount": 40,
        "encoding": "utf8",
        "delay": 0
      }
    }
  }
